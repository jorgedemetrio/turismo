{
    "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
    "name": "com_turismo",
    "version": "1.0.0",
    "description": "Assets for Tourism Component",
    "license": "GPL-2.0-or-later",
    "assets": [
        {
            "name": "com_turismo.site",
            "type": "style",
            "uri": "com_turismo/site.css",
            "dependencies": [
                "fontawesome"
            ]
        },
        {
            "name": "com_turismo.site",
            "type": "script",
            "uri": "com_turismo/site.js",
            "dependencies": [
                "core",
                "jquery"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.admin",
            "type": "style",
            "uri": "com_turismo/admin.css",
            "dependencies": [
                "fontawesome"
            ]
        },
        {
            "name": "com_turismo.admin",
            "type": "script",
            "uri": "com_turismo/admin.js",
            "dependencies": [
                "core",
                "jquery"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.maps",
            "type": "script",
            "uri": "com_turismo/maps.js",
            "dependencies": [
                "com_turismo.site"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.rating",
            "type": "script",
            "uri": "com_turismo/rating.js",
            "dependencies": [
                "com_turismo.site"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.gallery",
            "type": "script",
            "uri": "com_turismo/gallery.js",
            "dependencies": [
                "com_turismo.site"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.upload",
            "type": "script",
            "uri": "com_turismo/upload.js",
            "dependencies": [
                "com_turismo.admin"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.jquery.mask",
            "type": "script",
            "uri": "com_turismo/jquery.mask.min.js",
            "dependencies": [
                "jquery"
            ],
            "attributes": {
                "defer": true
            }
        },
        {
            "name": "com_turismo.validation",
            "type": "script",
            "uri": "com_turismo/validation.js",
            "dependencies": [
                "com_turismo.site",
                "com_turismo.jquery.mask"
            ],
            "attributes": {
                "defer": true
            }
        }
    ]
}
